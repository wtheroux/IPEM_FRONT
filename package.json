{
  "name": "ipem",
  "version": "1.0.0",
  "description": "information portal for employees and managers",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --debug services/",
    "lint:write": "eslint --debug services/ --fix",
    "prettier": "prettier --write services/**/*.ts|.tsx"
  },
  "author": "Makhmutov M.N.",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:write"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  }
}
